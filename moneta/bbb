<script>
  let totalClicks = localStorage.getItem('totalClicks') ? parseInt(localStorage.getItem('totalClicks')) : 0; /* –û–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ */
  const minWithdrawClicks = 1000000; /* –ú–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ –¥–ª—è –≤—ã–≤–æ–¥–∞ */
  let lastClickTime = parseInt(localStorage.getItem('lastClickTime')) || 0; /* –í—Ä–µ–º—è –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∫–ª–∏–∫–∞ */
  let questionNumber = 1; /* –ù–æ–º–µ—Ä —Ç–µ–∫—É—â–µ–≥–æ –≤–æ–ø—Ä–æ—Å–∞ */
  let correctAnswers = 0; /* –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */

  document.getElementById('counter').textContent = `DOSH COIN: ${totalClicks}`; /* –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç —Å—á–µ—Ç—á–∏–∫–∞ –∫–ª–∏–∫–æ–≤ */

  const questions = [ /* –ú–∞—Å—Å–∏–≤ –æ–±—ä–µ–∫—Ç–æ–≤ —Å –≤–æ–ø—Ä–æ—Å–∞–º–∏ –∏ –æ—Ç–≤–µ—Ç–∞–º–∏ */
    {
      question: "–ß—Ç–æ —Ç–∞–∫–æ–µ "—Ç—Ä–µ–π–¥–∏–Ω–≥" –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç?",
      answers: ["A) –ü—Ä–æ—Ü–µ—Å—Å –º–∞–π–Ω–∏–Ω–≥–∞ –Ω–æ–≤—ã—Ö –º–æ–Ω–µ—Ç", "B) –ü—Ä–æ—Ü–µ—Å—Å –ø–æ–∫—É–ø–∫–∏ –∏ –ø—Ä–æ–¥–∞–∂–∏ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç—ã —Å —Ü–µ–ª—å—é –ø—Ä–∏–±—ã–ª–∏", "C) –°–ø–æ—Å–æ–± —Ö—Ä–∞–Ω–µ–Ω–∏—è –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç—ã"],
      correctAnswer: "B"
    },
    {
      question: "–ö–∞–∫–∞—è –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–∞ –±—ã–ª–∞ –ø–µ—Ä–≤–æ–π –≤ –º–∏—Ä–µ?",
      answers: ["A) Bitcoin", "B) Ethereum", "C) Litecoin"],
      correctAnswer: "A"
    },
    {
      question: "–ö—Ç–æ —è–≤–ª—è–µ—Ç—Å—è —Å–æ–∑–¥–∞—Ç–µ–ª–µ–º –ë–∏—Ç–∫–æ–π–Ω–∞?",
      answers: ["A) –°–∞—Ç–æ—à–∏ –ù–∞–∫–∞–º–æ—Ç–æ", "B) –í–∏—Ç–∞–ª–∏–∫ –ë—É—Ç–µ—Ä–∏–Ω", "C) –≠–ª–∏—Å–æ–Ω –ì—Ä–∏–Ω—Å–ø–µ–Ω"],
      correctAnswer: "A"
    }
  ];

  const questions2 = [ /* –í—Ç–æ—Ä–æ–π –º–∞—Å—Å–∏–≤ –æ–±—ä–µ–∫—Ç–æ–≤ —Å –≤–æ–ø—Ä–æ—Å–∞–º–∏ –∏ –æ—Ç–≤–µ—Ç–∞–º–∏ */
    {
      question: "–ß—Ç–æ —Ç–∞–∫–æ–µ –º–∞–π–Ω–∏–Ω–≥ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç—ã?",
      answers: ["A) –ü—Ä–æ—Ü–µ—Å—Å —Å–æ–∑–¥–∞–Ω–∏—è –Ω–æ–≤—ã—Ö –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–Ω—ã—Ö –º–æ–Ω–µ—Ç", "B) –ü—Ä–æ—Ü–µ—Å—Å –ø—Ä–æ–≤–µ—Ä–∫–∏ –∏ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–π –≤ –±–ª–æ–∫—á–µ–π–Ω–µ", "C) –ü—Ä–æ—Ü–µ—Å—Å –æ–±–º–µ–Ω–∞ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç—ã –Ω–∞ —Ñ–∏–∞—Ç–Ω—ã–µ –¥–µ–Ω—å–≥–∏"],
      correctAnswer: "B"
    },
    {
      question: "–ß—Ç–æ —Ç–∞–∫–æ–µ ICO –≤ –∫–æ–Ω—Ç–µ–∫—Å—Ç–µ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç?",
      answers: ["A) –ò–Ω–∏—Ü–∏–∞—Ü–∏—è —Ü–∏—Ñ—Ä–æ–≤–æ–π –æ–ø–µ—Ä–∞—Ü–∏–æ–Ω–Ω–æ–π —Å–µ—Ç–∏", " B) –ò–Ω–∏—Ü–∏–∞–ª—å–Ω–æ–µ —Ü–∏—Ñ—Ä–æ–≤–æ–µ –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–µ", "C) –ò–Ω–≤–µ—Å—Ç–∏—Ü–∏–æ–Ω–Ω–∞—è –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–Ω–∞—è –æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è"],
      correctAnswer: "B"
    },
    {
      question: "–ö–∞–∫–∞—è –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–∞ –∏–∑–≤–µ—Å—Ç–Ω–∞ —Å–≤–æ–∏–º —É–Ω–∏–∫–∞–ª—å–Ω—ã–º –ø—Ä–æ—Ç–æ–∫–æ–ª–æ–º –¥–æ–∫–∞–∑–∞—Ç–µ–ª—å—Å—Ç–≤–∞ –¥–æ–ª–∏ (Proof of Stake)?",
      answers: ["A) Bitcoin", "B) Ethereum", "C) Cardano"],
      correctAnswer: "C"
    }
  ];

  function handleClick() {
    const currentTime = new Date().getTime(); /* –¢–µ–∫—É—â–µ–µ –≤—Ä–µ–º—è */
    const elapsedTime = currentTime - lastClickTime; /* –ü—Ä–æ—à–µ–¥—à–µ–µ –≤—Ä–µ–º—è —Å –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∫–ª–∏–∫–∞ */

    if (elapsedTime >= 1000 * 60 * 20) { /* –ï—Å–ª–∏ –ø—Ä–æ—à–ª–æ 20 –º–∏–Ω—É—Ç —Å –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∫–ª–∏–∫–∞ */
      document.getElementById('coin').style.cursor = 'pointer'; /* –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ã—á–Ω—ã–π –∫—É—Ä—Å–æ—Ä */
      document.getElementById('timer').style.display = 'none'; /* –°–∫—Ä—ã–≤–∞–µ–º —Ç–∞–π–º–µ—Ä */
    } else {
      const remainingTime = Math.ceil((1000 * 60 * 20 - elapsedTime) / 1000); /* –û—Å—Ç–∞–≤—à–µ–µ—Å—è –≤—Ä–µ–º—è –¥–æ —Å–ª–µ–¥—É—é—â–µ–≥–æ –∫–ª–∏–∫–∞ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö */
      document.getElementById('coin').style.cursor = 'not-allowed'; /* –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫—É—Ä—Å–æ—Ä "–Ω–µ–¥–æ—Å—Ç—É–ø–Ω–æ" */
      document.getElementById('timer').textContent = `Next click available in ${remainingTime} seconds`; /* –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç —Ç–∞–π–º–µ—Ä–∞ */
      document.getElementById('timer').style.display = 'block'; /* –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–∞–π–º–µ—Ä */
      return; /* –ü—Ä–µ–∫—Ä–∞—â–∞–µ–º –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏ */
    }

    document.getElementById('coin').style.transform = 'scale(1.1)'; /* –ê–Ω–∏–º–∞—Ü–∏—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –º–æ–Ω–µ—Ç—ã */
    setTimeout(() => {
      document.getElementById('coin').style.transform = 'scale(1)';
    }, 500); /* –ó–∞–¥–µ—Ä–∂–∫–∞ –ø–µ—Ä–µ–¥ –≤–æ–∑–≤—Ä–∞—â–µ–Ω–∏–µ–º –º–æ–Ω–µ—Ç—ã –≤ –∏—Å—Ö–æ–¥–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ */

    totalClicks++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ */
    updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */

    if (totalClicks === 1500) { /* –ï—Å–ª–∏ –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ —Ä–∞–≤–Ω–æ 1500 */
      askQuestion2(); /* –ó–∞–¥–∞–µ–º –Ω–æ–≤—ã–π —Ä–∞—É–Ω–¥ –≤–æ–ø—Ä–æ—Å–æ–≤ */
    } else if (totalClicks % 100 === 0 && correctAnswers < 3) { /* –ï—Å–ª–∏ –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ –∫—Ä–∞—Ç–Ω–æ 100 –∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
      askQuestion(); /* –ó–∞–¥–∞–µ–º –Ω–æ–≤—ã–π –≤–æ–ø—Ä–æ—Å */
    }

    localStorage.setItem('lastClickTime', currentTime); /* –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤—Ä–µ–º—è –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∫–ª–∏–∫–∞ –≤ localStorage */
  }

  function updateCounter() {
    document.getElementById('counter').textContent = `DOSH COIN: ${totalClicks}`; /* –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç —Å—á–µ—Ç—á–∏–∫–∞ –∫–ª–∏–∫–æ–≤ */
    localStorage.setItem('totalClicks', totalClicks); /* –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ –≤ localStorage */
    if (totalClicks >= minWithdrawClicks) { /* –ï—Å–ª–∏ –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ –¥–æ—Å—Ç–∏–≥–ª–æ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–≥–æ –¥–ª—è –≤—ã–≤–æ–¥–∞ */
      document.getElementById('cashOutButton').style.display = 'block'; /* –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –¥–ª—è –≤—ã–≤–æ–¥–∞ */
      document.getElementById('coin').style.display = 'none'; /* –°–∫—Ä—ã–≤–∞–µ–º –º–æ–Ω–µ—Ç—É */
    }
  }

  function askQuestion() {
    const question = questions[questionNumber - 1]; /* –¢–µ–∫—É—â–∏–π –≤–æ–ø—Ä–æ—Å */
    const answer = prompt(question.question + "\n" + question.answers.join(", ")); /* –ü–æ–ª—É—á–∞–µ–º –æ—Ç–≤–µ—Ç –æ—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è */
    if (answer && answer.toLowerCase() === question.correctAnswer.toLowerCase()) { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
      correctAnswers++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
      totalClicks += 300; /* –î–æ–±–∞–≤–ª—è–µ–º –±–æ–Ω—É—Å–Ω—ã–µ –∫–ª–∏–∫–∏ */
      updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
      alert("–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π! –í—ã –∑–∞—Ä–∞–±–æ—Ç–∞–ª–∏ 300 –ú–æ–Ω–µ—Ç üéÅ."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ */
      if (correctAnswers < 3) { /* –ï—Å–ª–∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
        questionNumber++; /* –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –≤–æ–ø—Ä–æ—Å—É */
      }
    } else { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
      alert("–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç. –£ –≤–∞—Å –æ—Å—Ç–∞–ª–æ—Å—å 2 –ø–æ–ø—ã—Ç–∫–∏."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ –∏ –æ—Å—Ç–∞–≤—à–∏—Ö—Å—è –ø–æ–ø—ã—Ç–∫–∞—Ö */
      const secondAttempt = prompt(question.question + "\n" + question.answers.join(", ")); /* –ü–æ–ª—É—á–∞–µ–º –≤—Ç–æ—Ä—É—é –ø–æ–ø—ã—Ç–∫—É –æ—Ç–≤–µ—Ç–∞ */
      if (secondAttempt && secondAttempt.toLowerCase() === question.correctAnswer.toLowerCase()) { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
        correctAnswers++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
        totalClicks += 300; /* –î–æ–±–∞–≤–ª—è–µ–º –±–æ–Ω—É—Å–Ω—ã–µ –∫–ª–∏–∫–∏ */
        updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
        alert("–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π! –í—ã –∑–∞—Ä–∞–±–æ—Ç–∞–ª–∏ 300 –ú–æ–Ω–µ—Ç üéÅ."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ */
        if (correctAnswers < 3) { /* –ï—Å–ª–∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
          questionNumber++; /* –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –≤–æ–ø—Ä–æ—Å—É */
        }
      } else { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
        alert("–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç. –£ –≤–∞—Å –æ—Å—Ç–∞–ª–∞—Å—å 1 –ø–æ–ø—ã—Ç–∫–∞. –ï—Å–ª–∏ –≤—ã —Å–Ω–æ–≤–∞ –ø–æ—Ç–µ—Ä–ø–∏—Ç–µ –Ω–µ—É–¥–∞—á—É, –≤–∞—à–∏ –º–æ–Ω–µ—Ç—ã –±—É–¥—É—Ç —Å–±—Ä–æ—à–µ–Ω—ã."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ –∏ –æ—Å—Ç–∞–≤—à–µ–π—Å—è –ø–æ–ø—ã—Ç–∫–µ */
        const thirdAttempt = prompt(question.question + "\n" + question.answers.join(", ")); /* –ü–æ–ª—É—á–∞–µ–º —Ç—Ä–µ—Ç—å—é –ø–æ–ø—ã—Ç–∫—É –æ—Ç–≤–µ—Ç–∞ */
        if (thirdAttempt && thirdAttempt.toLowerCase() === question.correctAnswer.toLowerCase()) { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
          correctAnswers++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
          totalClicks += 300; /* –î–æ–±–∞–≤–ª—è–µ–º –±–æ–Ω—É—Å–Ω—ã–µ –∫–ª–∏–∫–∏ */
          updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
          alert("–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π! –í—ã –∑–∞—Ä–∞–±–æ—Ç–∞–ª–∏ 300 –ú–æ–Ω–µ—Ç üéÅ."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ */
          if (correctAnswers < 3) { /* –ï—Å–ª–∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
            questionNumber++; /* –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –≤–æ–ø—Ä–æ—Å—É */
          }
        } else { /* –ï—Å–ª–∏ –∏ —Ç—Ä–µ—Ç–∏–π –æ—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
          alert("–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç. –í–∞—à–∏ –º–æ–Ω–µ—Ç—ã –±—É–¥—É—Ç —Å–±—Ä–æ—à–µ–Ω—ã."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ –∏ —Å–±—Ä–æ—Å–µ –∫–ª–∏–∫–æ–≤ */
          totalClicks = 0; /* –°–±—Ä–∞—Å—ã–≤–∞–µ–º –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ */
          updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
        }
      }
    }
  }

  function askQuestion2() { /* –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—Ç–æ—Ä–æ–≥–æ —Ä–∞—É–Ω–¥–∞ –≤–æ–ø—Ä–æ—Å–æ–≤ */
    questionNumber = 1; /* –°–±—Ä–∞—Å—ã–≤–∞–µ–º –Ω–æ–º–µ—Ä –≤–æ–ø—Ä–æ—Å–∞ */
    correctAnswers = 0; /* –°–±—Ä–∞—Å—ã–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
    const question = questions2[questionNumber - 1]; /* –¢–µ–∫—É—â–∏–π –≤–æ–ø—Ä–æ—Å */
    const answer = prompt(question.question + "\n" + question.answers.join(", ")); /* –ü–æ–ª—É—á–∞–µ–º –æ—Ç–≤–µ—Ç –æ—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è */
    if (answer && answer.toLowerCase() === question.correctAnswer.toLowerCase()) { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
      correctAnswers++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
      totalClicks += 300; /* –î–æ–±–∞–≤–ª—è–µ–º –±–æ–Ω—É—Å–Ω—ã–µ –∫–ª–∏–∫–∏ */
      updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
      alert("–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π! –í—ã –∑–∞—Ä–∞–±–æ—Ç–∞–ª–∏ 300 –ú–æ–Ω–µ—Ç üéÅ."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ */
      if (correctAnswers < 3) { /* –ï—Å–ª–∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
        questionNumber++; /* –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –≤–æ–ø—Ä–æ—Å—É */
      }
    } else { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
      alert("–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç. –£ –≤–∞—Å –æ—Å—Ç–∞–ª–æ—Å—å 2 –ø–æ–ø—ã—Ç–∫–∏."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ –∏ –æ—Å—Ç–∞–≤—à–∏—Ö—Å—è –ø–æ–ø—ã—Ç–∫–∞—Ö */
      const secondAttempt = prompt(question.question + "\n" + question.answers.join(", ")); /* –ü–æ–ª—É—á–∞–µ–º –≤—Ç–æ—Ä—É—é –ø–æ–ø—ã—Ç–∫—É –æ—Ç–≤–µ—Ç–∞ */
      if (secondAttempt && secondAttempt.toLowerCase() === question.correctAnswer.toLowerCase()) { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
        correctAnswers++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
        totalClicks += 300; /* –î–æ–±–∞–≤–ª—è–µ–º –±–æ–Ω—É—Å–Ω—ã–µ –∫–ª–∏–∫–∏ */
        updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
        alert("–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π! –í—ã –∑–∞—Ä–∞–±–æ—Ç–∞–ª–∏ 300 –ú–æ–Ω–µ—Ç üéÅ."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ */
        if (correctAnswers < 3) { /* –ï—Å–ª–∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
          questionNumber++; /* –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –≤–æ–ø—Ä–æ—Å—É */
        }
      } else { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
        alert("–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç. –£ –≤–∞—Å –æ—Å—Ç–∞–ª–∞—Å—å 1 –ø–æ–ø—ã—Ç–∫–∞. –ï—Å–ª–∏ –≤—ã —Å–Ω–æ–≤–∞ –ø–æ—Ç–µ—Ä–ø–∏—Ç–µ –Ω–µ—É–¥–∞—á—É, –≤–∞—à–∏ –º–æ–Ω–µ—Ç—ã –±—É–¥—É—Ç —Å–±—Ä–æ—à–µ–Ω—ã."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ –∏ –æ—Å—Ç–∞–≤—à–µ–π—Å—è –ø–æ–ø—ã—Ç–∫–µ */
        const thirdAttempt = prompt(question.question + "\n" + question.answers.join(", ")); /* –ü–æ–ª—É—á–∞–µ–º —Ç—Ä–µ—Ç—å—é –ø–æ–ø—ã—Ç–∫—É –æ—Ç–≤–µ—Ç–∞ */
        if (thirdAttempt && thirdAttempt.toLowerCase() === question.correctAnswer.toLowerCase()) { /* –ï—Å–ª–∏ –æ—Ç–≤–µ—Ç –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
          correctAnswers++; /* –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
          totalClicks += 300; /* –î–æ–±–∞–≤–ª—è–µ–º –±–æ–Ω—É—Å–Ω—ã–µ –∫–ª–∏–∫–∏ */
          updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
          alert("–ü—Ä–∞–≤–∏–ª—å–Ω—ã–π! –í—ã –∑–∞—Ä–∞–±–æ—Ç–∞–ª–∏ 300 –ú–æ–Ω–µ—ÇüéÅ."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ */
          if (correctAnswers < 3) { /* –ï—Å–ª–∏ –µ—â–µ –Ω–µ –±—ã–ª–æ —Ç—Ä–µ—Ö –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö –æ—Ç–≤–µ—Ç–æ–≤ */
            questionNumber++; /* –ü–µ—Ä–µ—Ö–æ–¥–∏–º –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –≤–æ–ø—Ä–æ—Å—É */
          }
        } else { /* –ï—Å–ª–∏ –∏ —Ç—Ä–µ—Ç–∏–π –æ—Ç–≤–µ—Ç –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π */
          alert("–ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –æ—Ç–≤–µ—Ç. –í–∞—à–∏ –º–æ–Ω–µ—Ç—ã –±—É–¥—É—Ç —Å–±—Ä–æ—à–µ–Ω—ã."); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ–º –æ—Ç–≤–µ—Ç–µ –∏ —Å–±—Ä–æ—Å–µ –∫–ª–∏–∫–æ–≤ */
          totalClicks = 0; /* –°–±—Ä–∞—Å—ã–≤–∞–µ–º –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ */
          updateCounter(); /* –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∫–ª–∏–∫–æ–≤ */
        }
      }
    }
  }

  document.getElementById('coin').addEventListener('click', handleClick); /* –î–æ–±–∞–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–ª–∏–∫–∞ –Ω–∞ –º–æ–Ω–µ—Ç—É */

  document.getElementById('cashOutButton').addEventListener('click', function() { /* –î–æ–±–∞–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–ª–∏–∫–∞ –Ω–∞ –∫–Ω–æ–ø–∫—É –≤—ã–≤–æ–¥–∞ */
    alert("–ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º! –í—ã –¥–æ—Å—Ç–∏–≥–ª–∏ 1 000 000 –∫–ª–∏–∫–æ–≤!üéÜ"); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –¥–æ—Å—Ç–∏–∂–µ–Ω–∏–∏ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–≥–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ –∫–ª–∏–∫–æ–≤ –¥–ª—è –≤—ã–≤–æ–¥–∞ */
  });

  document.getElementById('withdrawForm').addEventListener('submit', function(event) { /* –î–æ–±–∞–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Ñ–æ—Ä–º—ã –≤—ã–≤–æ–¥–∞ */
    event.preventDefault(); /* –ü—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–∞–µ–º –æ—Ç–ø—Ä–∞–≤–∫—É —Ñ–æ—Ä–º—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é */

    const withdrawAddress = document.getElementById('withdrawAddress').value; /* –ü–æ–ª—É—á–∞–µ–º –∞–¥—Ä–µ—Å –¥–ª—è –≤—ã–≤–æ–¥–∞ */

    if (totalClicks < minWithdrawClicks) { /* –ï—Å–ª–∏ –æ–±—â–µ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–ª–∏–∫–æ–≤ –º–µ–Ω—å—à–µ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–≥–æ –¥–ª—è –≤—ã–≤–æ–¥–∞ */
      alert('–î–ª—è –≤—ã–≤–æ–¥–∞ –º–æ–Ω–µ—Ç –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –Ω–µ –º–µ–Ω–µ–µ 1 000 000 .'); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏ –±–æ–ª—å—à–µ–≥–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ –∫–ª–∏–∫–æ–≤ */
    } else if (withdrawAddress === 'dosh_coin-sujnel.whjen12bjk') { /* –ï—Å–ª–∏ –∞–¥—Ä–µ—Å –¥–ª—è –≤—ã–≤–æ–¥–∞ –≤–µ—Ä–Ω—ã–π */
      alert('–ú–æ–Ω–µ—Ç—ã —É—Å–ø–µ—à–Ω–æ –≤—ã–≤–µ–¥–µ–Ω—ã!'); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ–± —É—Å–ø–µ—à–Ω–æ–º –≤—ã–≤–æ–¥–µ –º–æ–Ω–µ—Ç */
      document.getElementById('withdrawAddress').value = ''; /* –û—á–∏—â–∞–µ–º –ø–æ–ª–µ –¥–ª—è –∞–¥—Ä–µ—Å–∞ */
    } else { /* –ï—Å–ª–∏ –∞–¥—Ä–µ—Å –¥–ª—è –≤—ã–≤–æ–¥–∞ –Ω–µ–≤–µ—Ä–Ω—ã–π */
      alert('Incorrect withdrawal address! Please enter the correct address.'); /* –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –Ω–µ–≤–µ—Ä–Ω–æ–º –∞–¥—Ä–µ—Å–µ */
    }
  });

  setInterval(function() { /* –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∏–Ω—Ç–µ—Ä–≤–∞–ª –¥–ª—è —É–≤–µ–ª–∏—á–µ–Ω–∏—è –∫–ª–∏–∫–∞–±–µ–ª—å–Ω–æ—Å—Ç–∏ –∫–∞–∂–¥—ã–µ 20 –º–∏–Ω—É—Ç */
    const currentTime = new Date().getTime(); /* –¢–µ–∫—É—â–µ–µ –≤—Ä–µ–º—è */
    const elapsedTime = currentTime - lastClickTime; /* –ü—Ä–æ—à–µ–¥—à–µ–µ –≤—Ä–µ–º—è —Å –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∫–ª–∏–∫–∞ */
    if (elapsedTime >= 1000 * 60 * 20) { /* –ï—Å–ª–∏ –ø—Ä–æ—à–ª–æ 20 –º–∏–Ω—É—Ç —Å –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∫–ª–∏–∫–∞ */
      document.getElementById('coin').style.cursor = 'pointer'; /* –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ–±—ã—á–Ω—ã–π –∫—É—Ä—Å–æ—Ä */
      document.getElementById('timer').style.display = 'none'; /* –°–∫—Ä—ã–≤–∞–µ–º —Ç–∞–π–º–µ—Ä */
    } else {
      const remainingTime = Math.ceil((1000 * 60 * 20 - elapsedTime) / 1000); /* –û—Å—Ç–∞–≤—à–µ–µ—Å—è –≤—Ä–µ–º—è –¥–æ —Å–ª–µ–¥—É—é—â–µ–≥–æ –∫–ª–∏–∫–∞ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö */
      document.getElementById('coin').style.cursor = 'not-allowed'; /* –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫—É—Ä—Å–æ—Ä "–Ω–µ–¥–æ—Å—Ç—É–ø–Ω–æ" */
      document.getElementById('timer').textContent = `–°–ª–µ–¥—É—é—â–∏–π –∫–ª–∏–∫ –±—É–¥–µ—Ç—å –¥–æ—Å—Ç—É–ø–µ–Ω —á–µ—Ä–µ–∑  ${remainingTime} —Å–µ–∫—É–Ω–¥`; /* –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç —Ç–∞–π–º–µ—Ä–∞ */
      document.getElementById('timer').style.display = 'block'; /* –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–∞–π–º–µ—Ä */
    }
  }, 1000); /* –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–∞–∂–¥—É—é —Å–µ–∫—É–Ω–¥—É */

</script>

